FN Animal_method_constructor
self = load_param 0
esp = load_param 1
t0 = esp
	# Param 0 is reference to 'Self' for Animal
t1 = self + 0 	# Offset for Animal.especie
*t1 store t0 	# self.especie = t0
FN main
	# init Dog
*t2 = alloc 8 	# Allocating memory for Dog
push_param t2
t3 = "perro"
push_param t3
call Dog_method_constructor
	# finished init Dog
t4 = t2
d = t4
t5 = "Terrier"
t6 = d + 4 	# Offset for Dog.raza
*t6 store t5 	# d.raza = t5